<INPUT>
    <FILE type="1"><!-- 0 = training set 1 = testSet -->
        <NAME_FILE io="2">breast-cancer-wisconsin.data</NAME_FILE> <!-- io =0 input only, 1 output(label) only, 2 is Io -->
        <NUM_SAMPLE>299</NUM_SAMPLE>
        <CONVERT_METHOD>  <!-- 0 string to int, 1 string to double, 2 map table, 3 random(double) 4 random(int) -->
            <!-- *****for input which cannot regconizes,the sample is rejected immediately -->
            <METHOD max="10" min="1" key="?">4</METHOD> <!-- default method -->
        </CONVERT_METHOD>
        <SYMBOL>
            <LINE_START>400</LINE_START>
            <ORDER_ATTRB_START>1</ORDER_ATTRB_START>
            <IO_START>0</IO_START> <!--Optional tag use it when io=2 read 0 Input before output, 1 output before input -->
            <START_OUT>-1</START_OUT>
            <STOP_OUT>,</STOP_OUT>
            <START_INP>-1</START_INP>
            <STOP_INP>,</STOP_INP>
        </SYMBOL>
    </FILE>
</INPUT>